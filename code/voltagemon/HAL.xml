<?xml version="1.0" encoding="utf-8" ?>
<HAL>
	<templates>
		<template name="led">
			<pin type="output" initialvalue="inactive" />
			<activelow />
		</template>

		<template name="relais">
			<pin type="output" initialvalue="inactive" />
		</template>

		<template name="diginput">
			<pin type="input" initialpullup="active" />
		</template>
		
		<template name="diginputinv">
			<pin type="input" initialpullup="active" />
			<activelow />
		</template>
		
		<template name="digswitchinput">
			<pin type="input" initialpullup="active" />
			<activelow />
		</template>
		
		<template name="digoutput">
			<pin type="output" initialvalue="inactive" />
		</template>
		
		<template name="digoutputinv">
			<pin type="output" initialvalue="inactive" />
			<activelow />
		</template>
		
		<template name="digio">
			<pin type="io" initialstate="input" initialpullup="inactive" />
		</template>
	</templates>


	<portpin name="PB0" template="digoutput">
		<description value="SignalIRQ" />
	</portpin>
	
	<portpin name="PB4" template="digoutput">
		<description value="MISO" />
	</portpin>

	<portpin name="PB3" template="diginput">
		<description value="MOSI" />
	</portpin>

	<portpin name="PB2" template="diginput">
		<description value="SS" />
	</portpin>
	
	<portpin name="PB5" template="diginput">
		<description value="SCK" />
	</portpin>
	
	<portpin name="PC0" template="diginputinv"><description value="Sense01" /></portpin>
	<portpin name="PC1" template="diginputinv"><description value="Sense02" /></portpin>
	<portpin name="PC2" template="diginputinv"><description value="Sense03" /></portpin>
	<portpin name="PC3" template="diginputinv"><description value="Sense04" /></portpin>
	<portpin name="PC4" template="diginputinv"><description value="Sense05" /></portpin>
	<portpin name="PC5" template="diginputinv"><description value="Sense06" /></portpin>
	<portpin name="PD0" template="diginputinv"><description value="Sense07" /></portpin>
	<portpin name="PD1" template="diginputinv"><description value="Sense08" /></portpin>
	<portpin name="PD2" template="diginputinv"><description value="Sense09" /></portpin>
	<portpin name="PD3" template="diginputinv"><description value="Sense10" /></portpin>
	<portpin name="PD4" template="diginputinv"><description value="Sense11" /></portpin>
	<portpin name="PD5" template="diginputinv"><description value="Sense12" /></portpin>
	
	<portpin name="PD6" template="digoutput">
		<description value="LEDRed" />
	</portpin>
	
</HAL>
